        "tailwindcss": "^1.1.3",
        "tailwindcss-transitions": "^2.1.0",
        "vue-cli-plugin-storybook": "^0.6.1",
        "vue-functional-data-merge": "^3.1.0",
        "vue-template-compiler": "^2.6.10"
    }
