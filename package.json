{
    "name": "orto-ui",
    "version": "0.2.1",
    "private": false,
    "description": "Ui library based on Vue.js + Tailwindcss",
    "author": "matras-ru",
    "scripts": {
        "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "test:unit": "jest --clearCache && vue-cli-service test:unit",
        "lint": "vue-cli-service lint",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
        "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
        "dev": "vue-cli-service serve",
        "lint:fix": "vue-cli-service lint --fix",
        "storybook": "vue-cli-service storybook:serve -p 6006 -c config/storybook"
    },
    "main": "dist/orto-ui.ssr.js",
    "module": "dist/orto-ui.esm.js",
    "unpkg": "dist/orto-ui.min.js",
    "files": [
        "dist/*",
        "src/*",
        "public/*",
        "package.json",
        "*.js"
    ],
    "dependencies": {
        "@tailwindcss/custom-forms": "^0.2.1",
        "lodash.merge": "^4.6.2",
        "tailwindcss": "^1.1.4",
        "tailwindcss-transforms": "^2.2.0",
        "tailwindcss-transitions": "^2.1.0",
        "vue": "^2.6.11",
        "vue-functional-data-merge": "^3.1.0",
        "vue-router": "^3.1.3"
    },
    "devDependencies": {
        "@babel/plugin-proposal-optional-chaining": "^7.7.5",
        "@rollup/plugin-alias": "^3.0.0",
        "@rollup/plugin-buble": "^0.21.0",
        "@rollup/plugin-replace": "^2.3.0",
        "@storybook/addon-actions": "^5.2.8",
        "@storybook/addon-knobs": "^5.2.8",
        "@storybook/addon-links": "^5.2.8",
        "@storybook/addon-notes": "^5.2.8",
        "@storybook/addon-viewport": "^5.2.8",
        "@vue/cli-plugin-babel": "^4.1.1",
        "@vue/cli-plugin-eslint": "^4.1.1",
        "@vue/cli-plugin-unit-jest": "^4.1.1",
        "@vue/cli-service": "^4.1.1",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/test-utils": "1.0.0-beta.30",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.0.3",
        "cross-env": "^6.0.3",
        "eslint": "^6.8.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-vue": "^6.0.1",
        "minimist": "^1.2.0",
        "node-sass": "^4.13.0",
        "prettier": "^1.19.1",
        "rollup": "^1.27.14",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-terser": "^5.1.3",
        "rollup-plugin-vue": "5.1.4",
        "sass-loader": "^8.0.0",
        "svg-sprite-loader": "^4.1.6",
        "svgo": "^1.3.2",
        "svgo-loader": "^2.2.1",
        "vue-cli-plugin-component": "^1.12.2",
        "vue-cli-plugin-storybook": "^0.6.1",
        "vue-template-compiler": "^2.6.11"
    },
    "bugs": {
        "url": "https://github.com/matras-ru/orto-ui/issues"
    },
    "homepage": "https://github.com/matras-ru/orto-ui",
    "keywords": [],
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/matras-ru/orto.git"
    }
}
