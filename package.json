{
    "name": "orto-ui",
    "version": "0.1.1",
    "description": "Ui library based on Vue.js + Tailwindcss",
    "private": false,
    "keywords": [],
    "author": "matras-ru",
    "homepage": "https://github.com/matras-ru/orto-ui",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/matras-ru/orto.git"
    },
    "bugs": {
        "url": "https://github.com/matras-ru/orto-ui/issues"
    },
    "files": [
        "dist"
    ],
    "main": "dist/orto-ui.umd.min.js",
    "scripts": {
        "dev": "vue-cli-service serve",
        "build": "npm run lint && npm run test && vue-cli-service build --target lib src/index.js",
