{
    "name": "orto-ui",
    "version": "0.2.58",
    "private": false,
    "description": "Ui library based on Vue.js + Tailwindcss",
    "author": "matras-ru",
    "scripts": {
        "build": "npm run test:unit && cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "test:unit": "jest --clearCache && vue-cli-service test:unit",
        "lint": "vue-cli-service lint",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
        "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
        "dev": "vue-cli-service serve",
        "lint:fix": "vue-cli-service lint --fix",
        "storybook": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
        "storybook:build": "vue-cli-service storybook:build -c config/storybook",
        "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook"
    },
    "main": "dist/orto-ui.ssr.js",
    "module": "dist/orto-ui.esm.js",
    "unpkg": "dist/orto-ui.min.js",
    "files": [
        "dist/*",
        "src/*",
        "public/*",
        "package.json",
        "*.js"
    ],
    "dependencies": {
        "floating-vue": "^1.0.0-beta.19",
        "lodash": "^4.17.21",
        "mini-svg-data-uri": "^1.4.4",
        "v-click-outside-x": "^4.1.3",
        "vue": "^2.7.16",
        "vue-functional-data-merge": "^3.1.0"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.23.3",
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-buble": "^1.0.3",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.5",
        "@rollup/plugin-terser": "^0.4.4",
        "@storybook/addon-essentials": "^6.4.9",
        "@storybook/vue": "^6.4.9",
        "@vue/cli-plugin-babel": "^5.0.8",
        "@vue/cli-plugin-eslint": "^5.0.8",
        "@vue/cli-plugin-unit-jest": "^5.0.8",
        "@vue/cli-service": "^5.0.8",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/test-utils": "^1.3.6",
        "autoprefixer": "^10.4.17",
        "babel-core": "7.0.0-bridge.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.56.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-vue": "^9.20.1",
        "minimist": "^1.2.8",
        "postcss": "^8.4.33",
        "prettier": "^3.2.4",
        "rollup": "^4.9.6",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-visualizer": "^5.12.0",
        "rollup-plugin-vue": "6.0.0",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
        "tailwindcss-multi-column": "^1.0.2",
        "vue-cli-plugin-component": "^1.12.2",
        "vue-cli-plugin-storybook": "^2.1.0",
        "vue-router": "^3.5.3",
        "vue-template-compiler": "^2.7.16",
        "vuelidate": "^0.7.7"
    },
    "bugs": {
        "url": "https://github.com/matras-ru/orto-ui/issues"
    },
    "homepage": "https://github.com/matras-ru/orto-ui",
    "keywords": [],
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/matras-ru/orto.git"
    }
}
