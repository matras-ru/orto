{
    "name": "orto-ui",
    "version": "0.2.19",
    "private": false,
    "description": "Ui library based on Vue.js + Tailwindcss",
    "author": "matras-ru",
    "scripts": {
        "build": "npm run test:unit & cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "test:unit": "jest --clearCache && vue-cli-service test:unit",
        "lint": "vue-cli-service lint",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
        "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
        "dev": "vue-cli-service serve",
        "lint:fix": "vue-cli-service lint --fix",
        "storybook": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
        "storybook:build": "vue-cli-service storybook:build -c config/storybook",
        "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook"
    },
    "main": "dist/orto-ui.ssr.js",
    "module": "dist/orto-ui.esm.js",
    "unpkg": "dist/orto-ui.min.js",
    "files": [
        "dist/*",
        "src/*",
        "public/*",
        "package.json",
        "*.js"
    ],
    "dependencies": {
        "@tailwindcss/custom-forms": "^0.2.1",
        "lodash.merge": "^4.6.2",
        "tailwindcss": "^1.2.0",
        "v-click-outside-x": "^4.1.0",
        "vue": "^2.6.11",
        "vue-functional-data-merge": "^3.1.0"
    },
    "devDependencies": {
        "@babel/plugin-proposal-optional-chaining": "^7.9.0",
        "@rollup/plugin-alias": "^3.1.0",
        "@rollup/plugin-buble": "^0.21.3",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "@rollup/plugin-replace": "^2.3.1",
        "@storybook/addon-actions": ">=5.3.18",
        "@storybook/addon-knobs": ">=5.3.18",
        "@storybook/addon-links": ">=5.3.18",
        "@storybook/addon-notes": ">=5.3.18",
        "@storybook/addon-viewport": "^5.3.18",
        "@storybook/vue": ">=5.3.18",
        "@vue/cli-plugin-babel": "^4.3.1",
        "@vue/cli-plugin-eslint": "^4.3.1",
        "@vue/cli-plugin-unit-jest": "^4.3.1",
        "@vue/cli-service": "^4.3.1",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/test-utils": "1.0.0-beta.33",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.1.0",
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^6.2.2",
        "minimist": "^1.2.5",
        "node-sass": "^4.13.1",
        "postcss-custom-properties": "^9.1.1",
        "postcss-import": "^12.0.1",
        "postcss-nested": "^4.2.1",
        "prettier": "^2.0.4",
        "rollup": "^2.6.1",
        "rollup-plugin-analyzer": "^3.2.2",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-terser": "^5.3.0",
        "rollup-plugin-visualizer": "^4.0.4",
        "rollup-plugin-vue": "5.1.6",
        "sass-loader": "^8.0.2",
        "svgo": "^1.3.2",
        "svgo-loader": "^2.2.1",
        "vue-cli-plugin-component": "^1.12.2",
        "vue-cli-plugin-storybook": "^1.2.2",
        "vue-router": "^3.1.6",
        "vue-template-compiler": "^2.6.11",
        "vuelidate": "^0.7.5"
    },
    "bugs": {
        "url": "https://github.com/matras-ru/orto-ui/issues"
    },
    "homepage": "https://github.com/matras-ru/orto-ui",
    "keywords": [],
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/matras-ru/orto.git"
    }
}
